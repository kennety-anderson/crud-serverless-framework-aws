{
    "title": "Create user",
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "body"
    ],
    "properties": {
        "body": {
            "type": "object",
            "required": [
                "name",
                "email",
                "password",
                "cpf",
                "birthDate"
            ],
            "properties": {
                "name": {
                    "type": "string",
                    "minLength": 3
                },
                "email": {
                    "type": "string",
                    "format": "email"
                },
                "password": {
                    "type": "string",
                    "minLength": 8
                },
                "cpf": {
                    "type": "string",
                    "minLength": 11,
                    "maxLength": 11
                },
                "birthDate": {
                    "type": "string",
                    "format": "date"
                }
            }
        }
    }
}