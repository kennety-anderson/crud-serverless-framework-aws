{
    "title": "Upadte user",
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "body",
        "pathParameters"
    ],
    "properties": {
        "pathParameters": {
            "type": "object",
            "required": [
                "id"
            ]
        },
        "id": {
            "type": "string",
            "pattern": "^(.*)$"
        }
    },
    "body": {
        "type": "object",
        "properties": {
            "name": {
                "type": "string",
                "minLength": 3,
                "pattern": "^(.*)$"
            },
            "email": {
                "type": "string",
                "format": "email"
            },
            "password": {
                "type": "string",
                "minLength": 8
            },
            "cpf": {
                "type": "string",
                "minLength": 11,
                "maxLength": 11
            },
            "birthDate": {
                "type": "string",
                "format": "date"
            }
        }
    }
}